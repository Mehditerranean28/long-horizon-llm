{
  "name": "son-of-anton",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "ci": "npm ci && npm run validate",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "dev": "cross-env NODE_ENV=development next dev --turbopack -p 9002",
    "build": "cross-env NODE_ENV=production next build",
    "start": "pm2-runtime ecosystem.config.js --env production",
    "start:node": "cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --max-warnings=0 .",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:node",
    "test:node": "NODE_ENV=test node --test --test-concurrency=1 \"tests/node/*.test.js\"",
    "coverage": "c8 --reporter=text --reporter=lcov npm run test:node",
    "test:e2e": "npm run test:playwright && npm run test:cypress && npm run test:k6",
    "test:playwright": "playwright install --with-deps && playwright test",
    "test:cypress": "cypress run --headless",
    "test:k6": "k6 run --out json=reports/k6-summary.json scripts/k6.js",
    "test:lighthouse": "npm run build && lhci autorun --upload.token=$LHCI_TOKEN",
    "security": "npm audit --audit-level=high",
    "check:translations": "node ../scripts/check-translations.js",
    "fill:translations": "node ../scripts/checkTranslationsWithGPT.js",
    "setup": "bash scripts/dev/setup_frontend.sh",
    "generate-api": "bash ../scripts/generate_client.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "latest",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "latest",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-menubar": "latest",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-radio-group": "latest",
    "@radix-ui/react-scroll-area": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "latest",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@react-three/drei": "latest",
    "@react-three/fiber": "latest",
    "@stripe/stripe-js": "latest",
    "@tanstack/react-query": "latest",
    "@xenova/transformers": "latest",
    "bcryptjs": "latest",
    "class-variance-authority": "latest",
    "clsx": "latest",
    "compression": "^1.8.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "cross-env": "latest",
    "date-fns": "latest",
    "dotenv": "latest",
    "express": "latest",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "gsap": "latest",
    "helmet": "^8.1.0",
    "idb-keyval": "latest",
    "joi": "latest",
    "k6": "latest",
    "lucide-react": "latest",
    "mongodb": "latest",
    "mongoose": "^8.18.0",
    "next": "^15.5.2",
    "next-pwa": "latest",
    "nodemailer": "latest",
    "pino": "latest",
    "pm2": "latest",
    "prom-client": "latest",
    "react": "latest",
    "react-country-flag": "latest",
    "react-day-picker": "latest",
    "react-dom": "latest",
    "react-hook-form": "latest",
    "reactflow": "latest",
    "recharts": "latest",
    "stripe": "^18.5.0",
    "tailwind-merge": "latest",
    "tailwindcss-animate": "latest",
    "three": "latest",
