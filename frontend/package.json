{
  "name": "son-of-anton",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "ci": "npm ci && npm run validate",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "dev": "cross-env NODE_ENV=development next dev --turbopack -p 9002",
    "build": "cross-env NODE_ENV=production next build",
    "start": "pm2-runtime ecosystem.config.js --env production",
    "start:node": "cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --max-warnings=0 .",
    "typecheck": "tsc --noEmit",
    "test": "npm run test:node",
    "test:node": "NODE_ENV=test node --test --test-concurrency=1 \"tests/node/*.test.js\"",
    "coverage": "c8 --reporter=text --reporter=lcov npm run test:node",
    "test:e2e": "npm run test:playwright && npm run test:cypress && npm run test:k6",
    "test:playwright": "playwright install --with-deps && playwright test",
    "test:cypress": "cypress run --headless",
    "test:k6": "k6 run --out json=reports/k6-summary.json scripts/k6.js",
    "test:lighthouse": "npm run build && lhci autorun --upload.token=$LHCI_TOKEN",
    "security": "npm audit --audit-level=high",
    "check:translations": "node ../scripts/check-translations.js",
    "fill:translations": "node ../scripts/checkTranslationsWithGPT.js",
    "setup": "bash scripts/dev/setup_frontend.sh",
    "generate-api": "bash ../scripts/generate_client.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@stripe/stripe-js": "^2.1.0",
    "@tanstack/react-query": "^5.66.0",
    "@xenova/transformers": "^2.17.2",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "compression": "^1.7.4",
    "connect-mongo": "5.1.0",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^6.5.1",
    "express-session": "^1.18.1",
    "gsap": "^3.13.0",
    "helmet": "^7.0.0",
    "idb-keyval": "^6.2.1",
    "joi": "^17.13.3",
    "k6": "*",
    "lucide-react": "^0.475.0",
    "mongodb": "^5.8.0",
    "mongoose": "^8.15.1",
    "next": "^15.3.3",
    "next-pwa": "^5.6.0",
    "nodemailer": "^6.10.1",
    "pino": "^9.7.0",
    "pm2": "^6.0.8",
    "prom-client": "^15.1.3",
    "react": "^18.3.1",
    "react-country-flag": "^3.1.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "reactflow": "11.11.3",
    "recharts": "^2.15.1",
    "stripe": "^12.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.157.0",
    "uuid": "^9.0.1",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.7",
    "@eslint/eslintrc": "^2.1.4",
    "@lhci/cli": "^0.15.0",
    "@playwright/test": "^1.53.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "c8": "^8.0.1",
    "cypress": "^14.5.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "husky": "*",
    "k6": "*",
    "lint-staged": "^16.1.0",
    "mongodb-memory-server": "^10.1.4",
    "openai": "^4.38.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
